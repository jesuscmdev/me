{
   "buildCommand": "npm run build",
   "builds": [
      {
         "src": "build",
         "use": "@vercel/node"
      }
   ],
   "rewrites": [
      {
         "source": "/(.*)",
         "destination": "/"
      }
   ],
   "routes": [
      {
         "src": "/[^.]+",
         "dest": "/",
         "status": 200
      }
   ]
}
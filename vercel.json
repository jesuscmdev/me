{
   "version": 2,
   "builds": [
      {
         "src": "build/*",
         "use": "@vercel/static"
      }
   ],
   "routes": [
      {
         "handle": "filesystem"
      },
      {
         "src": "/.*",
         "dest": "/index.html"
      }
   ],
   "env": {
      "NODE_ENV": "production"
   }
}